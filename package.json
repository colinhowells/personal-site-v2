{
	"name": "personal-site-v2",
	"version": "2.6.0",
	"private": true,
	"type": "module",
	"engines": {
		"node": "22.x",
		"npm": "10.x"
	},
	"devDependencies": {
		"@sveltejs/adapter-cloudflare": "^7.2.4",
		"@sveltejs/kit": "^2.48.5",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@sveltejs/vite-plugin-svelte-inspector": "^5.0.1",
		"@types/node": "^24.10.1",
		"mdsvex": "^0.12.6",
		"prettier": "^3.6.2",
		"prettier-plugin-css-order": "^2.1.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prism-themes": "^1.9.0",
		"svelte": "^5.43.10",
		"svelte-check": "^4.3.4",
		"tslib": "^2.8.1",
		"typescript": "^5.9.3",
		"valibot": "^1.1.0",
		"vite": "^7.2.2"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"cf-typegen": "wrangler types",
		"cf-preview-prod": "npm run cf-typegen && npm run build && wrangler dev",
		"cf-preview-staging": "npm run cf-typegen && npm run build && wrangler dev --env staging"
	}
}
